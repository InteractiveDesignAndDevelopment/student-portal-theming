{
  "name": "student-portal-theming",
  "version": "0.0.1",
  "description": "A custom portal theme",
  "repository": "https://github.com/InteractiveDesignAndDevelopment/student-portal-theming.git",
  "scripts": {
    "prebuild": "run-s clean",
    "build": "run-s build:*",
    "build:scss": "run-p build:scss:*",
    "build:scss:portal-bootstrap": "node-sass scss/portal/bootstrap.scss out/portal/bootstrap.css",
    "build:scss:portal-bootstrap-custom": "node-sass scss/portal/bootstrap_custom.scss out/portal/bootstrap_custom.css",
    "build:scss:portal-main": "node-sass scss/portal/Main.scss out/portal/Main.css",
    "build:scss:student-bootstrap": "node-sass scss/student/bootstrap.scss out/student/bootstrap.css",
    "build:scss:student-bootstrap-custom": "node-sass scss/student/bootstrap_custom.scss out/student/bootstrap_custom.css",
    "build:scss:student-main": "node-sass scss/student/Main.scss out/student/Main.css",
    "postbuild": "run-s postbuild:*",
    "postbuild:post-css": "run-p postbuild:post-css:*",
    "postbuild:post-css:portal-bootstrap": "postcss out/portal/bootstrap.css -u autoprefixer -o out/portal/bootstrap.css",
    "postbuild:post-css:portal-bootstrap_custom": "postcss out/portal/bootstrap_custom.css -u autoprefixer postcss-safe-important -o out/portal/bootstrap_custom.css",
    "postbuild:post-css:portal-main": "postcss out/portal/main.css -u autoprefixer -o out/portal/main.css",
    "postbuild:post-css:student-bootstrap": "postcss out/student/bootstrap.css -u autoprefixer -o out/student/bootstrap.css",
    "postbuild:post-css:student-bootstrap_custom": "postcss out/student/bootstrap_custom.css -u autoprefixer postcss-safe-important -o out/student/bootstrap_custom.css",
    "postbuild:post-css:student-main": "postcss out/student/main.css -u autoprefixer -o out/student/main.css",
    "postbuild:csso": "run-p postbuild:csso:*",
    "postbuild:csso:portal-bootstrap": "csso out/portal/bootstrap.css out/portal/bootstrap.css",
    "postbuild:csso:portal-bootstrap-custom": "csso out/portal/bootstrap_custom.css out/portal/bootstrap_custom.css",
    "postbuild:csso:portal-main": "csso out/portal/Main.css out/portal/Main.css",
    "postbuild:csso:student-bootstrap": "csso out/student/bootstrap.css out/student/bootstrap.css",
    "postbuild:csso:student-bootstrap-custom": "csso out/student/bootstrap_custom.css out/student/bootstrap_custom.css",
    "postbuild:csso:student-main": "csso out/student/Main.css out/student/Main.css",
    "clean": "rimraf out && mkdirp out && mkdirp out/portal && mkdirp out/staff && mkdirp out/student",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Interactive Design & Development",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^7.1.5",
    "csso-cli": "^1.1.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "postcss-cli": "^4.1.1",
    "postcss-safe-important": "^1.0.6",
    "rimraf": "^2.6.2"
  }
}
